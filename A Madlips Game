void main(){


//Madlips game
print('Enter a color:');
String color = stdin.readLineSync();
print('Enter a food name:');
String food = stdin.readLineSync();
print('Enter a noun:');
String noun = stdin.readLineSync();
print('Enter a wrestler:');
String wrestler = stdin.readLineSync();

print('roses are ${color}');
print('${food} are blue');
print('you are a ${noun}');
print('and I\'m in love with ${wrestler}');
print('This is fun!');
}
